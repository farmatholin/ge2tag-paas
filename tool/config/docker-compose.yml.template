geo2tag_%container_id%:
  image: %image_container%
  ports:
    - "%port%:80"
  volumes:
    - %plugins_path%:/var/www/geomongo/src/plugins
    - %logs_path%:/var/log/apache2

  #memory control here;
  cpu_shares: %cpu_shares%
  cpu_quota: %cpu_quota%
  #cpuset: 0,1

  mem_limit: %mem_limit%
  #memswap_limit: 2000000000

  restart: always
  links:
    - mongo_%container_id%:mongo
mongo_%container_id%:
  image: mongo:latest
  volumes:
    - %db_path%:/data/db